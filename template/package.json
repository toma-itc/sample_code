    "@types/jquery": "^3.5.5",
    "@types/node": "^14.14.22",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "core-js": "^3.19.0",
    "fork-ts-checker-webpack-plugin": "^6.1.0",
    "json-loader": "^0.5.7",
    "json-schema-to-ts": "^1.5.0",
    "regenerator-runtime": "^0.13.7",
    "ts-loader": "^8.0.14",
