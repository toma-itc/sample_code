{
  "name": "template",
  "version": "1.0.0",
  "description": "",
  "main": "",
  "scripts": {
    "clean": "rimraf dist && rimraf dist-web && rimraf dist-node",
    "test-node": "run-s build-webpack-node-resolve-path-develop && node dist-node/sample/sample-node.js",
    "build-webpack-web-develop": "npx webpack-cli --mode development",
    "build-webpack-web-release": "npx webpack-cli --mode production",
    "build-webpack-web-none": "npx webpack-cli --mode none",
    "build-webpack-node-resolve-path-develop": "npx webpack-cli --config webpack.config.node.resolve.path.js --mode development",
    "build-webpack-node-resolve-path-release": "npx webpack-cli --config webpack.config.node.resolve.path.js --mode production",
    "build-webpack-commonlib-develop": "npx webpack-cli --config webpack.config.commonlib.js --mode development",
    "build-webpack-commonlib-release": "npx webpack-cli --config webpack.config.commonlib.js --mode production",
    "build-tsc-web.ds": "npx tsc -p tsconfig-web.json",
    "build-tsc-node": "npx tsc -p tsconfig-node.json",
    "build-tsc-web-t.ds": "npx tsc -p tsconfig-web.json --emitDeclarationOnly",
    "build-tsc-node-t.ds": "npx tsc -p tsconfig-node.json --emitDeclarationOnly",
    "license:check": "grunt"
  },
  "dependencies": {
    "grunt": "^1.4.1",
